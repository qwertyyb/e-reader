{
  "name": "e-reader",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "dev:android": "RUN_ENV=dev cap run android",
    "dev:ios": "RUN_ENV=dev cap run ios",
